{
  "name": "jesssullivan.github.io",
  "version": "0.0.1",
  "license": "CC0-1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "prebuild": "node scripts/generate-search-index.mjs && node scripts/optimize-images.mjs && node scripts/generate-blog-stats.mjs && node scripts/generate-tag-graph.mjs",
    "build": "vite build",
    "postbuild": "node scripts/generate-redirects.mjs && pagefind --site build",
    "test:redirects": "npm run build && node scripts/validate-redirects.mjs",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.2",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.15.0",
    "pagefind": "^1.4.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "serve": "^14.2.5",
    "sharp": "0.33.5",
    "svelte": "^5.19.0",
    "svelte-check": "^4.1.4",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.55.0",
    "vite": "^6.4.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@fontsource/fira-code": "^5.2.7",
    "@fontsource/fira-sans": "^5.2.7",
    "@fontsource/raleway": "^5.2.8",
    "@skeletonlabs/skeleton": "^4.12.0",
    "@skeletonlabs/skeleton-svelte": "^1.3.1",
    "flexsearch": "^0.8.212",
    "mdsvex": "^0.12.3",
    "mermaid": "^11.12.2",
    "shiki": "^3.22.0",
    "tailwindcss": "^4.0.0"
  },
  "type": "module",
  "overrides": {
    "sharp": "0.33.5"
  }
}
